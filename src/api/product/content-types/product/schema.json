{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "sortedNumber": {
      "type": "integer"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Open",
        "Closed"
      ],
      "default": "Open",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "userLimit": {
      "type": "boolean",
      "private": true,
      "required": true
    },
    "quantity": {
      "type": "integer",
      "required": true
    },
    "items": {
      "type": "component",
      "repeatable": true,
      "component": "raffle.items"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    }
  }
}
