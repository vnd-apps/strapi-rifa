{
  "collectionName": "components_raffle_items",
  "info": {
    "displayName": "Items",
    "description": ""
  },
  "options": {},
  "attributes": {
    "number": {
      "type": "integer",
      "unique": false,
      "required": true
    },
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Open",
        "Reserved",
        "Ordered",
        "Paid"
      ],
      "required": true,
      "default": "Open"
    },
    "reserved_at": {
      "type": "datetime"
    },
    "reserved_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
